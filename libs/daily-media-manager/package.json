{
  "name": "@pipecat-ai/react-native-daily-media-manager",
  "version": "0.0.1",
  "license": "BSD-2-Clause",
  "description": "React Native Daily Media manager",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/module/index.d.ts",
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "build": "bob build",
    "tag": "../../scripts/tag",
    "prepublishOnly": "npm run tag"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pipecat-ai/pipecat-client-react-native-transports.git"
  },
  "bugs": {
    "url": "https://github.com/pipecat-ai/pipecat-client-react-native-transports/issues"
  },
  "homepage": "https://github.com/pipecat-ai/pipecat-client-react-native-transports#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "peerDependencies": {
    "@daily-co/react-native-daily-js": "^0.82.0",
    "@daily-co/react-native-webrtc": "^124.0.6-daily.1",
    "@pipecat-ai/react-native-small-webrtc-transport": "*",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "react-native": "*",
    "react-native-background-timer": "^2.4.1",
    "react-native-get-random-values": "^1.11.0"
  },
  "devDependencies": {
    "@daily-co/react-native-daily-js": "^0.82.0",
    "@daily-co/react-native-webrtc": "^124.0.6-daily.1",
    "@pipecat-ai/react-native-small-webrtc-transport": "*",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "del-cli": "^6.0.0",
    "react-native-background-timer": "^2.4.1",
    "react-native-builder-bob": "^0.40.13",
    "react-native-get-random-values": "^1.11.0",
    "typescript": "*"
  }
}
